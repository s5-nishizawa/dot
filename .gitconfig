[color]
        status = auto
        diff = auto
        branch = auto
        interactive = auto
        grep = auto

[push]
  default = matching

[alias]
        co = checkout
        st = status -s
        ci = commit -a
        br = branch

[core]
  editor = vim -c \"set fenc=utf-8\"
  sparsecheckout = true
  excludesfile = ~/.gitignore
  fileMode = false
